
.bigBox {
	width: 100%;
	height: 100vh;
	overflow: hidden;
	position: relative;
}
.content {
	width: 100%;
	height: 100vh;
}
.bubbleBox {
	width: 100%;
	height: 100rpx;
	padding: 20rpx;
	position: relative;
}
.bubbleItem {
	border-radius: 50%;
	background: palegreen;
	display: flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	color: cadetblue;
	font-size: 24rpx;
	padding: 20rpx;
	-webkit-animation-direction: alternate;
	        animation-direction: alternate;
	-webkit-animation-timing-function: ease-in-out;
	        animation-timing-function: ease-in-out;
	position: absolute;
}
@-webkit-keyframes square1 {
0% {
		-webkit-transform: scale(1);
		        transform: scale(1)
}
25% {
		-webkit-transform: scale(1.2);
		        transform: scale(1.2)
}
50% {
		-webkit-transform: scale(1);
		        transform: scale(1)
}
75% {
		-webkit-transform: scale(1.2);
		        transform: scale(1.2)
}
100% {
		-webkit-transform: scale(1);
		        transform: scale(1)
}
}
